}

model WajibPajakStatus {
  id             Int        @id(map: "StatusAktif_pkey") @default(autoincrement())
  wajib_pajak_id Int
  createdAt      DateTime   @default(now())
  tanggal        DateTime
  alasan         String
  referensi      String?
  is_aktif       Boolean    @default(true)
  WajibPajak     WajibPajak @relation(fields: [wajib_pajak_id], references: [id], map: "StatusAktif_wajib_pajak_id_fkey")

  @@schema("pajak_general")
}

model SKPD {
  id                    Int        @id @default(autoincrement())
  nomor_urut            String?
