  @@schema("pajak_admin")
}

model JenisPajak {
  id                        Int                   @id @default(autoincrement())
  nama                      String
  nomor_rekening_pendapatan String?
  periode_masa              String?
  kode_pajak                String?
  hitung_sendiri            Boolean?
  tarif                     Float?
  KodeBayar                 KodeBayar[]
  ObjekPajak                ObjekPajak[]
